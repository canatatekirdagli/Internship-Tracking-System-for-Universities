@using MvcYazGelProje.Models.Entity
@model List<form>
@{
    ViewBag.Title = "StajBilgileri";
    Layout = "~/Views/Shared/OgrenciLayout.cshtml";
}

<div class="container col-md-9 pt-5">
    <div style="overflow-x:auto;">
        <table class="table table-striped text-white ">
            <tr style="background-color:grey">
                <th>Evrak No</th>
                <th>Başlangıç Tarih</th>
                <th>Bitiş Tarih</th>
                <th>Dönem</th>
                <th>Başarı Durumu</th>
                <th>Staj/IME</th>
                <th>Sorumlu Öğretmen</th>


                <th> </th>

            </tr>


            @foreach (var k in Model)
            {


                <tr>
                    <td>@k.staj_id</td>
                    <td>@k.staj_baslangic</td>
                    <td>@k.staj_bitis</td>
                    <td>@k.stajDonemiYıl @k.stajDonemiDonem</td>
                    <td>@k.stajNotu</td>
                    <td>@k.stajIME</td>
                    <td>@k.sorumlu</td>
                    <td>@k.basvuruDurumu</td>

                </tr>

            }




        </table>
    </div>
</div>


